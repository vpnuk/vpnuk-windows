{
  "name": "VPNUK",
  "author": {
    "name": "Mikhail Kryuchkov",
    "email": "kryuchkovm@outlook.com"
  },
  "version": "v1.0.0",
  "description": "VPNUK desktop application",
  "repository": {
    "type": "git",
    "url": "https://github.com/vpnuk/vpnuk-windows.git"
  },
  "main": "app/main/main.js",
  "config-overrides-path": "app/modules/config-overrides.js",
  "homepage": "./",
  "private": true,
  "fs": "empty",
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "cross-env CI='' react-app-rewired build",
    "react-test": "react-app-rewired test",
    "react-eject": "react-app-rewired eject",
    "electron-start": "electron .",
    "nsis-plugin-copy": "xcopy /e /h /i .\\nsis\\Plugins\\* .\\build\\",
    "build-react-nsis": "yarn run react-build && yarn run nsis-plugin-copy",
    "start": "concurrently --kill-others \"dotenv cross-var yarn run react-start\" \"wait-on http://localhost:3000 && dotenv cross-var electron .\"",
    "build": "yarn run build-react-nsis && dotenv cross-var yarn run electron-builder",
    "test": "dotenv cross-var echo %ELECTRON_ENV%"
  },
  "build": {
    "productName": "VPNUK",
    "appId": "net.vpnuk.desktop",
    "copyright": "Interexy LLC",
    "win": {
      "target": [
        {
          "target": "nsis-web",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/favicon.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "publish": [
        "github"
      ],
      "certificateSubjectName": "Online Privacy Services Ltd"
    },
    "nsisWeb": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerHeader": "nsis/installerHeader.bmp",
      "installerSidebar": "nsis/installerSidebar.bmp",
      "runAfterFinish": true,
      "include": "nsis/extension.nsh"
    },
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "electronDownload": {
      "cache": ".cache/electron"
    },
    "publish": {
      "provider": "github",
      "private": true
    },
    "files": [
      "build/**/*",
      "app/main/**/*",
      "app/modules/**/*",
      "app/assets/**/*"
    ],
    "extends": null
  },
  "dependencies": {
    "electron-store": "^7",
    "electron-updater": "^4.3.8",
    "public-ip": "^4.0.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "antd": "^3.21.2",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "customize-cra": "^0.4.1",
    "dotenv-cli": "^4.0.0",
    "electron": "^11",
    "electron-builder": "^22.10.5",
    "electron-fetch": "^1.7.3",
    "fs": "0.0.1-security",
    "mobx": "^6.2.0",
    "mobx-persist-store": "^0.5.30",
    "mobx-react-lite": "^3.2.0",
    "node-stream-zip": "^1.13.3",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.0",
    "react-select": "^4.1.0",
    "uuid": "^8.3.2",
    "wait-on": "^5.2.1"
  },
  "resolutions": {
    "**/app-builder-lib": "npm:app-builder-lib-mkfork@22.10.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
