{
  "name": "vpnuk",
  "author": {
    "name": "Mikhail Kryuchkov",
    "email": "kryuchkovm@outlook.com"
  },
  "version": "0.2.0",
  "description": "VPNUK desktop application",
  "main": "main.js",
  "config-overrides-path": "config-overrides.js",
  "homepage": "./",
  "private": true,
  "fs": "empty",
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-app-rewired test",
    "react-eject": "react-app-rewired eject",
    "electron-start": "electron .",
    "start": "concurrently --kill-others \"cross-env BROWSER=none yarn run react-start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_ENV=Dev electron .\"",
    "electron-build": "electron-builder",
    "build": "yarn run react-build && cross-env PORTABLE_EXECUTABLE_DIR=%APPDATA%\\Local\\vpnuk && yarn run electron-build"
  },
  "build": {
    "productName": "VPNUK",
    "appId": "net.vpnuk.desktop",
    "copyright": "Interexy LLC",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/favicon.ico",
      "publisherName": "Interexy LLC"
    },
    "nsis": {
      "artifactName": "vpnuk_installer.exe"
    },
    "portable": {
      "artifactName": "vpnuk_portable.exe"
    },
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/helpers/*",
      "src/settings/*",
      "main.js"
    ],
    "extends": null
  },
  "dependencies": {
    "@ant-design/icons": "4.0.0-rc.0",
    "@reduxjs/toolkit": "^1.5.0",
    "antd": "^3.21.2",
    "axios": "^0.21.1",
    "axios-electron-adapter": "^1.0.2",
    "cross-env": "^7.0.3",
    "electron-fetch": "^1.7.3",
    "electron-store": "^7.0.2",
    "fs": "0.0.1-security",
    "react": "^16.8.6",
    "react-burger-menu": "^2.7.1",
    "react-dom": "^16.8.6",
    "react-hook-form": "^6.15.1",
    "react-hook-form-input": "^1.1.10",
    "react-redux": "^7.2.2",
    "react-scripts": "3.0.1",
    "react-select": "^4.1.0",
    "react-show-more": "^2.0.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-electron-storage": "^2.1.0",
    "uuid": "^8.3.2",
    "write-file-p": "^1.0.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "babel-plugin-import": "^1.12.0",
    "concurrently": "^6.0.0",
    "customize-cra": "^0.4.1",
    "electron": "^10.0.0",
    "electron-builder": "^22.9.1",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^2.1.3",
    "wait-on": "^5.2.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
